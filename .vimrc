set nocompatible              " be iMproved, required
filetype off                  " required


set shell=bash
set nocompatible              " be iMproved, required
filetype off                  " required

set cindent
set expandtab
set shiftwidth=4
set softtabstop=4
set nocompatible  
syntax on       
set number        
set cursorline 
set ruler  
set shiftwidth=4 
set softtabstop=4 
set tabstop=4 
set nobackup 
map <C-L> :tabn <CR>
map <C-H> :tabp <CR>
map <C-H> :tabp <CR>
nmap :te :tabe


set nocompatible              " be iMproved, required
filetype off                  " required

filetype plugin indent on    " required

